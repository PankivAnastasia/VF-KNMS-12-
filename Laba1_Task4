num_1 = int(input("Enter first number:"))
num_2 = int(input("Enter second number:"))
znak = input("Enter +,-,* or /:")
if znak == "+":
    print(num_1 + num_2)
if znak == "-":
    print(num_1 - num_2)
if znak == "*":
    print(num_1 * num_2)
if znak == "/":
    if num_2 == 0:
        print("error: we can't divide by zero")
    else:
        print(num_1 / num_2)
print("\nThanks for use this calculator:)")
